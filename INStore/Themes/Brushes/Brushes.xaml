<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Brushs used-->
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="White"/>
    <SolidColorBrush x:Key="DefaultButtonColor" Color="Black"/>
    <SolidColorBrush x:Key="PrimarytButtonColor" Color="#BE1E2D"/>
    <SolidColorBrush x:Key="SecondaryButtonColor" Color="#333333"/>
    <SolidColorBrush x:Key="BackgroundBorderBrush"
                     Color="#333333" Opacity="0.96"/>
    <SolidColorBrush x:Key="MenuBorderBrush"
                     Color="#333333"
                     Opacity="0.96"/>
    <SolidColorBrush x:Key="MenuBorderBrush2"
                     Color="#333333"/>
