    <SolidColorBrush x:Key="PrimarytButtonColor" Color="#BE1E2D"/>
    <SolidColorBrush x:Key="SecondaryButtonColor" Color="#333333"/>
    <SolidColorBrush x:Key="BackgroundBorderBrush" Color="Black" Opacity="0.6"/>
    <SolidColorBrush x:Key="MenuBorderBrush" Color="Black" Opacity="0.95"/>
    <SolidColorBrush x:Key="BorderBorderBrush" Color="Black" Opacity="0.9"/>
