    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#808080" />
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="White"/>
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#000000 "/>
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#000000"/>
